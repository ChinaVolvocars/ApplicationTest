<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/bt_main_pressed" />
            <corners android:radius="@dimen/height_y_5" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/bt_main_disable" />
            <corners android:radius="@dimen/height_y_5" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/bt_main_normal" />
            <corners android:radius="@dimen/height_y_5" />
        </shape>
    </item>
</selector>