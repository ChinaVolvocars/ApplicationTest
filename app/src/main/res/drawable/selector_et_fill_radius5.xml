<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/height_y_5" />
            <stroke android:width="1dp" android:color="@color/et_fill_focused" />
            <solid android:color="@color/et_fill_bg"/>
        </shape>
    </item>

    <item android:state_focused="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/height_y_5" />
            <stroke android:width="1dp" android:color="@color/et_fill_bg" />
            <solid android:color="@color/et_fill_bg"/>
        </shape>
    </item>

</selector>